<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="390.0" prefWidth="520.0" stylesheets="@../MyStyles.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AllStudentsViewController">
   <children>
      <TextArea fx:id="allStudentsArea" editable="false" prefColumnCount="50" prefRowCount="16" />
      <Button fx:id="getButton" mnemonicParsing="false" onAction="#handleActions" text="Get Students">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin></Button>
   </children>
   <padding>
      <Insets bottom="5.0" />
   </padding>
</VBox>
